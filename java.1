public class Solution 
{
    public void moveZeroes(int[] nums) 
	{
        int n = nums.length;
        int left = 0; 
        for (int i = 0; i < n; i++) 
		{
            if (nums[i] != 0) 
			{
                nums[left] = nums[i];
                left++;
            }
        }
        while (left < n) {
            nums[left] = 0;
            left++;
        }
    }
}